//Binary Search tree
//BST Insertion

//BStree.h file Defines our ADT
typedef struct Node *Tree;

//BStree.c file Implements the tree
#include "BStree.h"
typedef struct Node {
  int data;
  Tree left, right;
} Node;

//main.c
int main(int argc, char *argv[]) {

  return 0;
}

//Tute 3
//Palendrome activity
int main(int argc, char *argv[]) {
  char *word = argv[1];  
  for (int l = 0; r= strlen(word) - 1; l < r; l++; r--) {
    if (word[l] != word[r]) {
      printf("no\n");
      return 0;
    }
    printf("yes\n");
    return 0;
  }
}










